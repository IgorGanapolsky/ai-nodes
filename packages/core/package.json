{
  "name": "@depinautopilot/core",
  "version": "1.0.0",
  "description": "Core domain models, types, and business logic for DePIN Autopilot",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "pnpm exec tsc",
    "dev": "tsc --watch",
    "type-check": "tsc --noEmit",
    "lint": "eslint .",
    "test": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "date-fns": "4.1.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/node": "^24.8.1",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "@vitest/coverage-v8": "^4.0.3",
    "eslint": "^9.38.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.3"
  },
  "keywords": [
    "depin",
    "core",
    "domain",
    "models",
    "types"
  ],
  "engines": {
    "node": ">=18.0.0"
  }
}
